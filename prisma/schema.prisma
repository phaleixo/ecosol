generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Service {
  id        Int      @id @default(autoincrement())
  name      String
  category  String
  image     String?
  whatsapp  String?
  instagram String?
  tiktok    String?
  email     String?
  site      String?
  approved  Boolean  @default(false)
  suspended Boolean  @default(false)
  createdAt DateTime @default(now())
}
